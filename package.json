{
  "name": "bitcoin-net",
  "version": "4.1.2",
  "description": "Bitcoin P2P networking tht works in Node and the browser",
  "main": "index.js",
  "scripts": {
    "test": "standard src test index.js && nyc tape test/*.js | faucet; nyc report",
    "test-browser": "npm run build && test/browser.sh; npm run source",
    "build": "rm -rf lib && babel --presets es2015 src -d lib",
    "source": "rm -rf lib && ln -s src lib",
    "prepublish": "npm run build",
    "publish": "npm run source"
  },
  "keywords": [
    "bitcoin",
    "networking",
    "p2p",
    "bitcore"
  ],
  "author": "Matt Bell <mappum@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "async": "^1.5.2",
    "bitcoin-merkle-proof": "0.0.x",
    "bitcoin-protocol": "^3.0.0",
    "bitcoin-util": "^2.0.0",
    "bitcoinjs-lib": "^2.2.0",
    "bn.js": "^4.11.1",
    "buffer-reverse": "^1.0.1",
    "debug": "^2.2.0",
    "event-cleanup": "^1.0.3",
    "get-browser-rtc": "^1.0.2",
    "once": "^1.3.3",
    "peer-exchange": "^0.5.0",
    "pumpify": "^1.3.4",
    "setimmediate": "^1.0.4",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "blockchain-spv": "^2.0.0",
    "envify": "^3.4.0",
    "faucet": "0.0.1",
    "flush-write-stream": "^1.0.0",
    "levelup": "^1.3.1",
    "memdown": "^1.1.2",
    "nyc": "^6.4.4",
    "standard": "^7.0.1",
    "tape": "^4.5.1",
    "webcoin-bitcoin": "^2.0.0",
    "zuul": "^3.10.1"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mappum/bitcoin-net.git"
  },
  "bugs": {
    "url": "https://github.com/mappum/bitcoin-net/issues"
  },
  "homepage": "https://github.com/mappum/bitcoin-net#readme",
  "browserify": {
    "transform": [
      "envify"
    ]
  }
}
