    "bitcoin-merkle-proof": "0.0.x",
    "bitcoin-protocol": "^3.0.0",
    "bitcoin-util": "^2.0.0",
    "bitcoinjs-lib": "^2.2.0",
    "bn.js": "^4.11.1",
    "buffer-reverse": "^1.0.1",
    "debug": "^2.2.0",
    "get-browser-rtc": "^1.0.2",
    "once": "^1.3.3",
    "peer-exchange": "^0.4.0",
    "pumpify": "^1.3.4",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
