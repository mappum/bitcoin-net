{
  "name": "bitcoin-net",
  "version": "3.2.0",
  "description": "Bitcoin P2P networking tht works in Node and the browser",
  "main": "index.js",
  "scripts": {
    "test": "standard src test index.js && tap -Rspec -t300 --cov test/*.js",
    "build": "rm -rf lib && babel --presets es2015 src -d lib",
